<StringItem>:
  height: 60
  size_hint_y: None
  BoxLayout:
    orientation: "horizontal"
    Button:
      size_hint_x: None
      width: self.height
      font_name: "fonts/Font Awesome 6 Free-Solid-900.otf"
      font_size: 21
      text: "\uf129"
#      on_release: Factory.Wifi().open()
    Label:
      size_hint_x: 0.7
      text: root.name
    TextInput:
      size_hint_x: None
      width: 250
      multiline: False
      text: root.value
      font_size: 24
      valign: "center"
      halign: "center"
      disabled: root.disabled
      on_text_validate: root.value = self.text