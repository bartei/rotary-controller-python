#: import PlotToolbar rcp.components.plot.plot_toolbar
#: import FloatView rcp.components.plot.float_view

<ScenePopup>:
    title: "Plot View"
    size_hint: 1, 1
    auto_dismiss: False
    background_color: 0,0,0,1
    toolbar: toolbar
    float_view: float_view

    BoxLayout:
        orientation: "vertical"
        FloatView:
            id: float_view
        PlotToolbar:
            id: toolbar
            float_view: root.float_view
            popup_instance: root
